%h2= t(".sign_up")
= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
  = devise_error_messages!
  %div.form_input
    = f.label :email
    = f.email_field :email
  %div.form_input
    = f.label :first_name
    = f.text_field :first_name
  %div.form_input
    = f.label :last_name
    = f.text_field :last_name
  %div.form_input
    = f.label :password
    %span.error_mes_pass
    = f.password_field :password, onblur: 'password_val($(this).val());'
  %div.form_input
    = f.label :password_confirmation
    = f.password_field :password_confirmation
  %div= f.submit t(".sign_up")
= render partial: "devise/shared/links"
